{
	"name": "unocss-preset-daisyui-next",
	"version": "0.1.1",
	"description": "UnoCSS preset updated for daisyUI 4",
	"scripts": {
		"prepare": "unbuild",
		"start": "vite demo --host",
		"lint": "biome ci src/**/*.ts *.d.ts",
		"check": "biome check src/**/*.ts *.d.ts"
	},
	"repository": "@dan-online/unocss-preset-daisyui-next",
	"license": "MIT",
	"author": "DanCodes <dan@dancodes.online>",
	"contributors": [
		"DanCodes <dan@dancodes.online>",
		"Adam Matthiesen <https://matthiesen.xyz>"
	],
	"keywords": [
		"unocss",
		"unocss-preset",
		"daisyui",
		"daisyui 4",
		"tailwindcss",
		"tailwindcss-preset"
	],
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.mts",
			"default": "./dist/index.mjs"
		},
		"./*": "./*"
	},
	"main": "dist/index.mjs",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"files": [
		"*.d.ts",
		"dist"
	],
	"dependencies": {
		"@unocss/core": "^65.4.0",
		"autoprefixer": "^10.4.20",
		"camelcase": "^8.0.0",
		"daisyui": "^4.12.23",
		"parsel-js": "^1.2.1",
		"postcss": "^8.4.49",
		"postcss-js": "^4.0.1"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@iconify/json": "^2.2.311",
		"@sindresorhus/tsconfig": "^7.0.0",
		"@types/postcss-js": "^4.0.4",
		"@unocss/reset": "^65.5.0",
		"typescript": "^5.8.2",
		"unbuild": "^3.5.0",
		"unocss": "^65.5.0",
		"vite": "^6.2.0"
	},
	"packageManager": "yarn@4.7.0"
}
